	<queryString><![CDATA[SELECT organization_unit_desc, rg.description as research_group, tg.description, isemployee FROM VIEW_ACTIVE_PERSONAL AP INNER JOIN PROFESSIONAL P ON AP.PERSONALCODE=P.PROFESSIONAL_PERSONAL INNER JOIN VIEW_ORGANIZATIONUNIT_UNIT vouu
  on p.professional_unit=vouu.unitcode
inner join grant_concession gc on gc.grant_concession_personal=ap.personalcode
inner join table_grant tg on gc.table_grant = tg.grantcode
inner join research_group rg on rg.research_groupcode=p.research_group
where
	p.current=1
and tg.type_of_grant='00001'
and vouu.organization_unitcode='00001']]></queryString>

