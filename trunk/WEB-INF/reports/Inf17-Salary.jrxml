	<queryString><![CDATA[SELECT  organization_unit_desc, unit_desc, c.amount, 1 FROM VIEW_ACTIVE_PERSONAL AP INNER JOIN PROFESSIONAL P ON AP.PERSONALCODE=P.PROFESSIONAL_PERSONAL INNER JOIN VIEW_ORGANIZATIONUNIT_UNIT vouu
  on p.professional_unit=vouu.unitcode inner join compensation c on c.compensation_personal = p.professional_personal
