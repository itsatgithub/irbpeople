                <field name="status" visible="true" default="true" type-of-field="text" size="15">
                    <caption language="en">Status</caption>
                </field>
            </visible-fields>
            <!-- - Campos NO visibles - NO MODIFICAR, ES SOLO A NIVEL INFORMATIVO -->
            <!-- 1/2 - Campos 'hidden'. -->
            <hidden-fields />
            <!-- 2/2 - Campos 'discarded'. -->
            <discarded-fields />
